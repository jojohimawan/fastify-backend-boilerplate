{
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "proseWrap": "always",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": ["*.ts", "*.js", "*.json"],
      "options": {
        "parser": "typescript"
      }
    }
  ]
}
